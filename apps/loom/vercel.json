{
    "redirects": [
        {
            "source": "/docs",
            "destination": "/docs/get-started"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/docs/(.*)",
            "destination": "/docs/index.html"
        }
    ],
    "trailingSlash": false
}
