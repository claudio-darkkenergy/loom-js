{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": "",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "paths": {
            "@data/*": [
                "data/*"
            ],
            "@app/*": [
                "src/*"
            ],
            "@framework/*": [
                "src/framework/*"
            ],
            "@framework": [
                "src/framework/index"
            ]
        },
        "resolveJsonModule": true,
        "sourceMap": true,
        "target": "ESNext"
    }
}
