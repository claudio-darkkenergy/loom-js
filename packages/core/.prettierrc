{
    "bracketSpacing": true,
    "importOrder": [
        "^[./]"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "noMultipleEmptyLines": true,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "overrides": [
        {
            "files": "*.mjs",
            "options": {
                "editorconfig": false,
                "parser": "typescript"
            }
        }
    ]
}
