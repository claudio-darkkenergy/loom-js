{
  "name": "@loom-js/pink",
  "version": "0.0.15",
  "author": "Claudio Nunez Jr",
  "license": "ISC",
  "description": "A design system based on Pink Design & made for loomjs.",
  "keywords": [
    "loomjs design system",
    "pink design"
  ],
  "exports": {
    "import": "./src/index.ts",
    "require": "./src/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darkkenergy/loomjs/packages/pink"
  },
  "scripts": {
    "build": "pnpm build-storybook",
    "build-package": "rollup -c rollup.config.ts --configPlugin typescript",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs",
    "dev": "pnpm storybook",
    "format": "prettier",
    "storybook": "storybook dev --no-open -p 6006",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@appwrite.io/pink": "^0.25.0",
    "@appwrite.io/pink-icons": "^0.25.0",
    "@loom-js/core": "workspace:*",
    "@loom-js/tags": "workspace:*",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@loom-js/storybook": "workspace:*",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/html": "^8.2.8",
    "@storybook/html-vite": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@types/node": "^20.14.10",
    "esbuild": "^0.23.0",
    "esbuild-node-externals": "^1.14.0",
    "rollup": "^4.18.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "storybook": "^8.2.8",
    "tslib": "^2.6.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "peerDependencies": {
    "@loom-js/core": "0.4.1",
    "@loom-js/tags": "0.0.15"
  }
}
